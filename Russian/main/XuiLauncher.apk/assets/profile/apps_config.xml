    <!-- 用于定义DOCK区 -->
    <dock>
        <app title="拨号">
            <item title="拨号" intent="#Intent;action=android.intent.action.DIAL;launchFlags=0x10000000;end" 
                icon_package="com.zui.launcher" icon_resource="com.zui.launcher:drawable/com_android_contacts__ic_launcher_phone"/>
        </app>
        <app title="联系人">
            <item title="联系人" intent="content://com.android.contacts/contacts#Intent;action=android.intent.action.VIEW;launchFlags=0x10000000;end" 
                icon_package="com.zui.launcher" icon_resource="com.zui.launcher:drawable/com_android_contacts__ic_launcher_contacts"/>
        </app>
        <app title="信息">
            <item title="信息" intent="#Intent;action=android.intent.action.MAIN;type=vnd.android-dir/mms-sms;launchFlags=0x10000000;end" 
                icon_package="com.zui.launcher" icon_resource="com.zui.launcher:drawable/com_android_mms__ic_launcher_smsmms"/>
        </app>        
        <app title="浏览器">
            <item title="浏览器" intent="http://m.idea123.cn/?c=02#Intent;action=android.intent.action.VIEW;S.com.android.browser.application_id=-2036735900831708960;end" 
                icon_package="com.zui.launcher" icon_resource="com.zui.launcher:drawable/com_android_browser__ic_launcher_browser"/>
        </app>
	</dock>
	
